package sec_verify.exam09;

import java.io.File;

public class FileDelete {

	public static void main(String[] args) {
		
		String path = "/Users/yunsangil/Downloads/*txt";
		File f = new File(path);
		if(f.delete()) {
			 System.out.println(path +" 삭제");
		 }
		else
			System.out.println("파일이 없습니다.");
	}
}
